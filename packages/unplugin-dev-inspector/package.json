{
  "name": "@mcpc-tech/unplugin-dev-inspector-mcp",
  "version": "0.1.22",
  "description": "Universal dev inspector plugin for React/Vue - inspect component sources and API calls in any bundler",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcpc-tech/dev-inspector-mcp.git",
    "directory": "packages/unplugin-dev-inspector"
  },
  "bugs": {
    "url": "https://github.com/mcpc-tech/dev-inspector-mcp/issues"
  },
  "homepage": "https://github.com/mcpc-tech/dev-inspector-mcp#readme",
  "keywords": [
    "mcp",
    "inspector",
    "debug",
    "ai",
    "agent",
    "react",
    "vue",
    "vite",
    "webpack",
    "nextjs",
    "devtools",
    "unplugin"
  ],
  "bin": {
    "dev-inspector-server": "./dist/cli.js",
    "dev-inspector": "./dist/cli.js"
  },
  "files": [
    "dist",
    "client/dist",
    "client.d.ts"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./next": {
      "import": {
        "types": "./dist/next.d.ts",
        "default": "./dist/next.js"
      },
      "require": {
        "types": "./dist/next.d.cts",
        "default": "./dist/next.cjs"
      }
    },
    "./client": {
      "types": "./client.d.ts"
    },
    "./loader": {
      "import": {
        "types": "./dist/loader.d.ts",
        "default": "./dist/loader.js"
      },
      "require": {
        "types": "./dist/loader.d.cts",
        "default": "./dist/loader.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "pnpm build:plugin && pnpm build:client",
    "build:plugin": "tsdown",
    "build:client": "tsdown --config tsdown.client.config.ts",
    "dev:plugin": "tsdown --watch",
    "dev:client": "tsdown --config tsdown.client.config.ts --watch",
    "lint": "oxlint .",
    "lint:fix": "oxlint --fix .",
    "format": "oxfmt .",
    "format:check": "oxfmt --check .",
    "prepublishOnly": "cp ./README.md ../../ && pnpm build"
  },
  "peerDependencies": {
    "esbuild": ">=0.17.0",
    "rollup": ">=3.0.0",
    "vite": ">=4.0.0",
    "webpack": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    }
  },
  "dependencies": {
    "@agentclientprotocol/sdk": "^0.5.1",
    "@ai-sdk/react": "^2.0.95",
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@code-inspector/core": "^1.3.0",
    "@mcpc-tech/acp-ai-provider": "^0.1.49",
    "@mcpc-tech/cmcp": "^0.0.15",
    "@mcpc-tech/core": "^0.3.8",
    "@modelcontextprotocol/sdk": "^1.20.1",
    "@mswjs/interceptors": "^0.40.0",
    "@radix-ui/primitive": "^1.1.3",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-compose-refs": "^1.1.2",
    "@radix-ui/react-context": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dismissable-layer": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-focus-guards": "^1.1.3",
    "@radix-ui/react-focus-scope": "^1.1.7",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-id": "^1.1.1",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-portal": "^1.1.9",
    "@radix-ui/react-presence": "^1.1.5",
    "@radix-ui/react-primitive": "^2.1.3",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "@radix-ui/react-visually-hidden": "^1.2.4",
    "@vue/compiler-sfc": "^3.5.25",
    "@xyflow/react": "^12.9.3",
    "ai": "^5.0.95",
    "aria-hidden": "^1.2.6",
    "chrome-devtools-mcp": "^0.12.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "embla-carousel-react": "^8.6.0",
    "html-to-image": "^1.11.13",
    "lucide-react": "^0.546.0",
    "magic-string": "^0.30.21",
    "mcp-remote": "^0.1.31",
    "motion": "^12.23.24",
    "nanoid": "^5.1.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-remove-scroll": "^2.7.1",
    "shiki": "^3.15.0",
    "sonner": "^2.0.7",
    "streamdown": "^1.5.1",
    "svelte": "^5.45.8",
    "tailwind-merge": "^3.3.1",
    "tokenlens": "^1.3.1",
    "unplugin": "^2.3.10",
    "use-stick-to-bottom": "^1.1.1",
    "zod": "^3.24.1"
  },
  "optionalDependencies": {
    "@blowmage/cursor-agent-acp": "^0.1.0",
    "@yaonyan/droid-acp": "^0.0.8",
    "@zed-industries/claude-code-acp": "^0.12.4",
    "@zed-industries/codex-acp": "^0.7.1"
  },
  "devDependencies": {
    "@babel/types": "^7.28.5",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "esbuild": "^0.25.11",
    "oxfmt": "^0.17.0",
    "oxlint": "^1.32.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "tsdown": "^0.15.6",
    "typescript": "^5.9.3",
    "vite": "^7.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}